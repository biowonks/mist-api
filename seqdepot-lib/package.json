    "sequelize-migrator": "https://github.com/lukeulrich/sequelize-migrator.git",
    "sequelize-model-loader": "https://github.com/lukeulrich/sequelize-model-loader.git",
    "split": "1.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^6.8.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
